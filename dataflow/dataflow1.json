{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlStudentTable",
						"type": "DatasetReference"
					},
					"name": "azuresource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlStudentRespTable",
						"type": "DatasetReference"
					},
					"name": "sinkstudentresp"
				}
			],
			"transformations": [
				{
					"name": "restapi",
					"linkedService": {
						"referenceName": "studentapi",
						"type": "LinkedServiceReference"
					}
				}
			],
			"scriptLines": [
				"source(output(",
				"          studentid as integer,",
				"          courseid as integer,",
				"          status as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     procedureName: 'uspGetStudent',",
				"     schemaName: 'dbo',",
				"     resultSet: true,",
				"     format: 'procedure') ~> azuresource",
				"azuresource call(mapColumn(",
				"          studentid,",
				"          courseid,",
				"          status",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     allowSchemaDrift: true,",
				"     format: 'rest',",
				"     store: 'restservice',",
				"     timeout: 30,",
				"     httpMethod: 'POST',",
				"     requestFormat: ['type' -> 'json'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine']) ~> restapi",
				"azuresource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          studentid as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinkstudentresp"
			]
		}
	}
}